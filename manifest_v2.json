{
    "manifest_version": 2,
    "version": "1",
    "name": "Pop the web",
    "browser_action": {
        "default_icon": "pin_128_128_crossed.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "pop.mp3"
    ]
}
